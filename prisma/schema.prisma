generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model blueprint {
  id    String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name  String
  souls Int?
  cost Int?
}

model user {
  id    String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  email  String
  password String
}